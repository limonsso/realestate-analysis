# üè† Variables d'Environnement pour le Test Chambly Plex
# Copier ce fichier vers .env.chambly_test et l'adapter selon votre environnement

# Configuration MongoDB
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=real_estate_test
MONGODB_COLLECTION=chambly_plex_test

# Configuration Centris
CENTRIS_BASE_URL=https://www.centris.ca
CENTRIS_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36

# Configuration Pipeline
MAX_WORKERS=2
BATCH_SIZE=5
REQUEST_TIMEOUT=60
MAX_RETRIES=5
RETRY_DELAY=2

# Configuration Logging
LOG_LEVEL=DEBUG
LOG_FILE=logs/chambly_test.log
LOG_FORMAT=json

# Configuration des Prix pour Chambly
SALE_PRICE_MIN=200000
SALE_PRICE_MAX=600000

# Configuration Test
TEST_TIMEOUT=300
MAX_TEST_PROPERTIES=10
MAX_TEST_PAGES=3
